{
    "name": "webfucktory/laravel-permissions",
    "require": {
        "php": "^8.2",
        "illuminate/support": "^10"
    },
    "require-dev": {
        "orchestra/testbench": "^8",
        "phpstan/phpstan": "^1",
        "squizlabs/php_codesniffer": "^3"
    },
    "autoload": {
        "psr-4": {
            "Webfucktory\\LaravelPermissions\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Webfucktory\\LaravelPermissions\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Webfucktory\\LaravelPermissions\\ServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    }
}
